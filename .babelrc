{
  "presets": [
    [
      "@babel/preset-react", 
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/preset-env", 
      {
        "modules": "umd",
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "ie >= 9"]
        },
        "corejs":{ "version": 3, "proposals": true },
        "useBuiltIns" : "entry"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ]
  ]
}